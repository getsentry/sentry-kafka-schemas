{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$ref": "#/definitions/Main",
  "definitions": {
    "Main": {
      "title": "subscription_result",
      "type": "object",
      "additionalProperties": false,
      "properties": {
        "version": {
          "const": 3
        },
        "payload": {
          "type": "object",
          "title": "payload_v3",
          "additionalProperties": false,
          "properties": {
            "subscription_id": {
              "type": "string",
              "minLength": 1
            },
            "request": {
              "type": "object"
            },
            "result": {
              "type": "object",
              "properties": {
                "data": {
                  "type": "array",
                  "items": {
                    "type": "object"
                  }
                },
                "meta": {
                  "type": "array",
                  "items": {
                    "type": "object",
                    "properties": {
                      "name": {
                        "type": "string"
                      },
                      "type": {
                        "type": "string"
                      }
                    },
                    "additionalProperties": false
                  }
                },
                "profile": {
                  "type": ["object", "null"]
                },
                "trace_output": {
                  "type": "string"
                }
              },
              "required": ["data", "meta"],
              "additionalProperties": true
            },
            "timestamp": {
              "type": "string"
            },
            "entity": {
              "type": "string",
              "minLength": 1
            }
          },
          "required": [
            "subscription_id",
            "request",
            "result",
            "timestamp",
            "entity"
          ]
        }
      },
      "required": ["version", "payload"]
    }
  }
}
