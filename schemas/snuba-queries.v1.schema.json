{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "querylog",
  "description": "Querylog schema",
  "type": "object",
  "properties": {
    "request": {
      "type": "object",
      "properties": {
        "id": {
          "type": "string",
          "pattern": "[0-9a-fA-F]{32}"
        },
        "body": {
          "type": "object"
        },
        "referrer": {
          "type": "string"
        },
        "app_id": {
          "type": "string"
        },
        "team": {
          "type": ["string", "null"]
        },
        "feature": {
          "type": ["string", "null"]
        }
      },
      "required": ["id"]
    },
    "dataset": {
      "type": "string"
    },
    "entity": {
      "type": "string"
    },
    "start_timestamp": {
      "type": ["integer", "null"]
    },
    "end_timestamp": {
      "type": ["integer", "null"]
    },
    "status": {
      "type": "string"
    },
    "request_status": {
      "type": "string"
    },
    "slo": {
      "type": "string"
    },
    "projects": {
      "type": "array",
      "items": {
        "type": "integer"
      }
    },
    "query_list": {
      "type": "array",
      "items": {
        "$ref": "#/definitions/QueryMetadata"
      }
    },
    "timing": {
      "title": "timer_data",
      "type": "object",
      "properties": {
        "timestamp": {
          "type": "integer"
        },
        "duration_ms": {
          "type": "integer"
        },
        "marks_ms": {
          "type": "object",
          "additionalProperties": {
            "type": "integer"
          }
        },
        "tags": {
          "type": "object",
          "additionalProperties": {
            "type": "string"
          }
        }
      },
      "required": ["timestamp", "duration_ms"],
      "additionalProperties": true
    },
    "snql_anonymized": {
      "type": "string"
    },
    "organization": {
      "type": ["integer", "null"]
    }
  },
  "required": [
    "request",
    "dataset",
    "entity",
    "start_timestamp",
    "end_timestamp",
    "status",
    "request_status",
    "slo",
    "projects",
    "query_list",
    "timing"
  ],
  "additionalProperties": true,
  "definitions": {
    "QueryMetadata": {
      "type": "object",
      "title": "query_metadata",
      "properties": {
        "sql": {
          "type": "string"
        },
        "sql_anonymized": {
          "type": "string"
        },
        "start_timestamp": {
          "type": ["integer", "null"]
        },
        "end_timestamp": {
          "type": ["integer", "null"]
        },
        "stats": {
          "type": "object",
          "properties": {
            "final": {
              "type": "boolean"
            },
            "cache_hit": {
              "type": "integer"
            },
            "sample": {
              "type": ["number", "null"]
            },
            "max_threads": {
              "type": "integer"
            },
            "clickhouse_table": {
              "type": "string"
            },
            "query_id": {
              "type": "string"
            },
            "is_duplicate": {
              "type": "integer"
            },
            "consistent": {
              "type": "boolean"
            }
          }
        },
        "status": {
          "type": "string"
        },
        "trace_id": {
          "type": ["string", "null"]
        },
        "profile": {
          "$ref": "#/definitions/ClickhouseQueryProfile"
        },
        "result_profile": {
          "type": ["object", "null"]
        },
        "request_status": {
          "type": "string"
        },
        "slo": {
          "type": "string"
        }
      },
      "required": [
        "sql",
        "sql_anonymized",
        "start_timestamp",
        "end_timestamp",
        "stats",
        "status",
        "trace_id",
        "profile",
        "result_profile",
        "request_status",
        "slo"
      ],
      "additionalProperties": true
    },
    "ColumnList": {
      "type": "array",
      "items": {
        "type": "string"
      }
    },
    "ClickhouseQueryProfile": {
      "type": "object",
      "title": "clickhouse_query_profile",
      "properties": {
        "time_range": {
          "type": ["integer", "null"]
        },
        "table": {
          "type": "string"
        },
        "all_columns": {
          "$ref": "#/definitions/ColumnList"
        },
        "multi_level_condition": {
          "type": "boolean"
        },
        "where_profile": {
          "title": "clickhouse_query_profile_where_profile",
          "type": "object",
          "properties": {
            "columns": {
              "$ref": "#/definitions/ColumnList"
            },
            "mapping_cols": {
              "$ref": "#/definitions/ColumnList"
            }
          },
          "required": ["columns", "mapping_cols"],
          "additionalProperties": true
        },
        "groupby_cols": {
          "$ref": "#/definitions/ColumnList"
        },
        "array_join_cols": {
          "$ref": "#/definitions/ColumnList"
        }
      },
      "required": [
        "time_range",
        "multi_level_condition",
        "where_profile",
        "groupby_cols",
        "array_join_cols"
      ]
    }
  }
}
