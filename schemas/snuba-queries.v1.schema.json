{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "title": "Querylog",
    "description": "Querylog schema",
    "type": "object",
    "properties": {
        "request": {
            "type": "object",
            "properties": {
                "id": {
                    "type": "string"
                },
                "body": {
                    "type": "object"
                },
                "referrer": {
                    "type": "string"
                }
            }
        },
        "dataset": {
            "type": "string"
        },
        "entity": {
            "type": "string"
        },
        "start_timestamp": {
            "type": [
                "integer",
                "null"
            ]
        },
        "end_timestamp": {
            "type": [
                "integer",
                "null"
            ]
        },
        "status": {
            "type": "string"
        },
        "projects": {
            "type": "array",
            "items": {
                "type": "integer"
            }
        },
        "query_list": {
            "type": "array",
            "items": {
                "$ref": "#/definitions/QueryMetadata"
            }
        },
        "timing": {
            "type": "object",
            "properties": {
                "timestamp": {
                    "type": "integer"
                },
                "duration_ms": {
                    "type": "integer"
                },
                "marks_ms": {
                    "type": "object"
                },
                "tags": {
                    "type": "object"
                }
            },
            "additionalProperties": false
        },
        "snql_anonymized": {
            "type": "string"
        }
    },
    "required": [
        "request",
        "dataset",
        "entity",
        "start_timestamp",
        "end_timestamp",
        "status",
        "projects",
        "query_list",
        "timing"
    ],
    "additionalProperties": false,
    "definitions": {
        "QueryMetadata": {
            "type": "object",
            "properties": {
                "sql": {
                    "type": "string"
                },
                "sql_anonymized": {
                    "type": "string"
                },
                "start_timestamp": {
                    "type": [
                        "integer",
                        "null"
                    ]
                },
                "end_timestamp": {
                    "type": [
                        "integer",
                        "null"
                    ]
                },
                "stats": {
                    "type": "object"
                },
                "status": {
                    "type": "string"
                },
                "trace_id": {
                    "type": [
                        "string",
                        "null"
                    ]
                },
                "profile": {
                    "type": "object"
                },
                "result_profile": {
                    "type": ["object", "null"]
                }
            },
            "required": [
                "sql",
                "sql_anonymized",
                "start_timestamp",
                "end_timestamp",
                "stats",
                "status",
                "trace_id",
                "profile",
                "result_profile"
            ],
            "additionalProperties": false
        },
        "ColumnList": {
            "type": "array",
            "items": {
                "type": "string"
            }
        },
        "ClickhouseQueryProfile": {
            "type": "object",
            "properties": {
                "time_range": {
                    "type": [
                        "integer",
                        "null"
                    ]
                },
                "table": {
                    "type": "string"
                },
                "all_columns": {
                    "$ref": "#/definitions/ColumnList"
                },
                "multi_level_condition": {
                    "type": "boolean"
                },
                "where_profile": {
                    "type": "object",
                    "properties": {
                        "columns": {
                            "$ref": "#/definitions/ColumnList"
                        },
                        "mapping_cols": {
                            "$ref": "#/definitions/ColumnList"
                        }
                    },
                    "required": [
                        "columns",
                        "mapping_cols"
                    ],
                    "additionalProperties": false
                },
                "groupby_cols": {
                    "$ref": "#/definitions/ColumnList"
                },
                "array_join_cols": {
                    "$ref": "#/definitions/ColumnList"
                }
            }
        }
    }
}
