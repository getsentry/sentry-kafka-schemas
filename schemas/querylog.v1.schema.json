{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "querylog",
  "description": "Querylog schema",
  "type": "object",
  "properties": {
      "request": {
          "type": "object",
          "properties": {
              "id": {
                  "type": "string"
              },
              "body": {
                  "type": "object"
              },
              "referrer": {
                  "type": "string"
              }
          }
      },
      "dataset": {
          "type": "string"
      },
      "projects": {
          "type": "array",
          "items": {
              "type": "integer"
          }
      },
      "query_list": {
          "type": "array",
          "items": {
              "type": "object"
          }
      }
  },
  "required": ["request", "dataset"],
  "additionalProperties": false
}
